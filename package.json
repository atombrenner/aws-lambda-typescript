{
  "name": "aws-lambda-typescript",
  "version": "1.0.0",
  "description": "AWS Lambda with Typescript",
  "license": "GPL-3.0-only",
  "repository": "github:atombrenner/aws-lambda-typescript",
  "scripts": {
    "housekeeping": "ts-node -T src/housekeeping/start.ts",
    "test": "jest",
    "clean": "rm -rf ./dist",
    "build": "esbuild --bundle --minify --keep-names --sourcemap --sources-content=false --target=node18 --platform=node --outfile=dist/index.js src/index.ts",
    "zip": "ts-node -T infrastructure/zip.ts",
    "dist": "tsc && npm test && npm run build && npm run zip",
    "deploy": "ts-node -T infrastructure/deploy.ts",
    "prepare": "husky install",
    "stack": "ts-node -T infrastructure/stack.ts"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@atombrenner/cfn-stack": "^2.0.2",
    "@aws-sdk/client-lambda": "^3.218.0",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.10.4",
    "@tsconfig/node18": "^1.0.0",
    "@types/adm-zip": "^0.5.0",
    "@types/aws-lambda": "^8.10.97",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "adm-zip": "^0.5.9",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.0",
    "esbuild": "^0.15.16",
    "husky": "^8.0.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.9.0"
  }
}
